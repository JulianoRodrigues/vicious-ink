{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "strict": 0,
        "no-console": "warn",
        "react/prop-types": 0,
        "react/display-name": {
            "ignoreTranspilerName": true
        },
        "quotes": [
            "warn",
            "single"
          ],
          "prettier/prettier": "warn"
    },
    "extends": [
      "plugin:react/recommended"
    ],
    "plugins": ["prettier"]
}